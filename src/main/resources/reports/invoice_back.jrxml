    <parameter name="flight_booking_id" class="java.lang.Long"/>
    <queryString language="SQL">
        <![CDATA[SELECT
			flight_booking.id as 'bookingId',
			flight_booking.passenger_first_name as 'firstName',
			flight_booking.passenger_last_name as 'lastName',
			flight_booking.passenger_passport_no as 'passportNo',
			flight_booking.passenger_mobile_no as 'mobileNo',
			flight_booking.email_address as 'email',
			REPLACE(flight_booking.booking_type, '_', ' ') as 'bookingType',
			flight_booking.pax_no as 'paxNo',
			flight_booking.pnr as 'pnr',
			flight_booking.e_ticket_no as 'eTicket',
			cast(flight_booking.total as decimal(10,2)) as 'total',
			IFNULL(flight_booking.fare_rules, '--') as 'fareRules',
			flight_booking.issued_at as 'issuedAt',
			ticket_itinerary.flight_no as 'flightNo',
			ticket_itinerary.airline_text as 'AirlineName',
			ticket_itinerary.departure_airport_text as 'departureAirport',
			ticket_itinerary.departure_date_time as 'departureTime',
			ticket_itinerary.arrival_airport_text as 'arrivalAirport',
			ticket_itinerary.arrival_date_time as 'arrivalDate',
			ticket_itinerary.cabin_class as 'cabinClass',
			ticket_itinerary.ticket_class as 'ticketClass',
			IFNULL(ticket_itinerary.bags, '--') as 'bags',
			IFNULL(ticket_itinerary.terminal, '--') as 'terminal',
			IFNULL(ticket_itinerary.gate, '--') as 'gate',
			IFNULL(ticket_itinerary.duration, '--') as 'duration'
			FROM flight_booking, ticket_itinerary
			WHERE flight_booking.id = ticket_itinerary.flight_booking_id
			and flight_booking.id = $P{flight_booking_id}]]>
    </queryString>
